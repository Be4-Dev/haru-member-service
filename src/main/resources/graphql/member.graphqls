type Query {
    member(id: ID): MemberDTO
}

type Mutation {
    registerMember(command: RegisterMemberCommand): MemberDTO
    updateMemberProfile(command: UpdateMemberProfileCommand): MemberDTO
}

input RegisterMemberCommand {
    nickname    : String
    email       : String
    password    : String
    createdBy   : String
}

input UpdateMemberProfileCommand {
    memberId    : Int
    nickname    : String
    updatedBy   : String
}